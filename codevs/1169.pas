const  
  inp='message.in';     oup='message.out';   var     a:array[1..50,1..50] of longint;     f:array[0..200,1..100,1..100] of longint;     tmp,i,j,k,n,m,d,x1,x2,y1,y2:longint;    
  function max(a,b:longint):longint;    begin       if a>b then exit(a);       exit(b); 
   end;    begin       readln(n,m);     for i:= 1 to n do       begin         for j:= 1 to m do           read(a[i,j]);         readln;       end;      fillchar(f,sizeof(f),0);     f[0,1,1]:=a[1,1];      f[1,1,2]:=a[2,1]+a[1,2]; f[1,2,1]:=a[1,2]+a[2,1];      for d:= 2 to  (n+m-3) do       for x1:= 1 to n do         begin            y1:=(d+2)-x1;             if (y1<=0) or (y1>m) then continue;          for x2:= 1 to n do              begin  
             if x1=x2 then continue;             y2:=(d+2) - x2;                if (y2<=0) or (y2>m) then continue;              tmp:=f[d-1,x1,x2];                              if (x1-1>0) and (x2-1>0)  then                   tmp:=max(tmp,f[d-1,x1-1,x2-1]);                                if (x1-1>0) and (x1-1<>x2)  then                   tmp:=max(tmp,f[d-1,x1-1,x2]);                                 if (x2-1>0) and (x1<>x2-1) then                   tmp:=max(tmp,f[d-1,x1,x2-1]);                             f[d,x1,x2]:=tmp+a[x1,y1]+a[x2,y2];              end;         end;     write(f[n+m-3,n,n-1]);  end.
